    a-entity preactivate-tooltip="Ortho Camera" camera-tool="orthographic: true" position="0.4 0.2 0" tooltip-style="scale: 0.3 0.3 1.0; offset: 0 -0.2 -1.3"
    a-entity.clickable preactivate-tooltip="Camera Track Target" camera-target="" geometry="primitive: dodecahedron; radius: 0.08" material="color: #fcba03; wireframe: true" manipulator-weight="weight: 0.7; type: slow" position="0.6 0.2 0" tooltip-style="scale: 0.3 0.3 1.0; offset: 0 -0.2 -1.3"
    a-entity preactivate-tooltip="Spray Paint" spray-can-tool="" position="0.0 -0.1 0" tooltip-style="scale: 0.3 0.3 1.0; offset: 0 -0.35 -1.3"
    a-entity preactivate-tooltip="Spray Projector" spray-can-tool="projector: true" position="0.4 -0.1 0" tooltip-style="scale: 0.3 0.3 1.0; offset: 0 -0.35 -1.3"
    a-entity preactivate-tooltip="Material Projector" spray-can-tool="projector: true; materialProjector: true" position="0.7 -0.1 0" tooltip-style="scale: 0.3 0.3 1.0; offset: 0 -0.35 -1.3"
  a-entity#misc-six-dof-tools visible="false"
    a-entity preactivate-tooltip="Color Picker" eye-drop-tool="" position="0 0.2 0" tooltip-style="scale: 0.3 0.3 1.0; offset: 0 -0.3 0"
    a-entity preactivate-tooltip="Viewport Positioner" viewport-tool="" position="0.4 0.2 0" tooltip-style="scale: 0.3 0.3 1.0; offset: 0 -0.1 -1.3"
    a-entity preactivate-tooltip="Movement Tool" movement-tool="" position="-0.25 0.2 0" tooltip-style="scale: 0.3 0.3 1.0; offset: 0 -0.1 -1.3"
    a-entity preactivate-tooltip="Move Selector" selection-box-tool="" position="0 -0.2 0"
